package com.java8.examples;

interface Maths{
	int operation(int a,int b);
}

public class Lambda {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Maths m1 = new Maths() {
			
			@Override
			public int operation(int a, int b) {
				// TODO Auto-generated method stub
				return a+b;
			}
		};
	}

}
