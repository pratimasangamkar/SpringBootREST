package com.java8.examples;

interface TestLocal{
	void show();
}

public class SpecialCaseLambda {

	int x = 10;
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		testLambdaLocal();
	}
	
	public void testLambdaLocal(){
		
		int y = 100;
		TestLocal t = () ->{
			System.out.println("x:"+x);
			
		};
		
	}

}
